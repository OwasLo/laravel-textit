{
    "name": "owaslo/textit",
    "description": "Laravel notification channel for textit Sri Lanka",
    "homepage": "https://github.com/laravel-notification-channels/textit",
    "license": "MIT",
    "authors": [{
        "name": "Upeksha Liyanage",
        "email": "upeksha@owaslo.com",
        "homepage": "https://www.owaslo.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Owaslo\\Textit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Owaslo\\Textit\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Owaslo\\Textit\\TextitServiceProvider"
            ]
        }
    }
}
